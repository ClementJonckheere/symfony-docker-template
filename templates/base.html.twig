      <li class="nav-item">
        <a class="nav-link" href="{{ path('app_home') }} ">Home</a>
      </li>
      <li class="nav-item">
        <a class="nav-link" href="{{ path('app_a_propos') }}">A propos</a>
      </li>
      <li class="nav-item">
        <a class="nav-link" href="{{ path('app_produits') }}">Produits</a>
      </li>
      <li class="nav-item">
        <a class="nav-link" href="{{ path('app_contact') }}">Contact</a>
